# Copier ce fichier en .env et remplir les valeurs
# cp .env.example .env

# Clé maître Rails (contenu du fichier config/master.key)
RAILS_MASTER_KEY=your_master_key_here

# Mot de passe PostgreSQL pour l'utilisateur 'achat'
ACHAT_DATABASE_PASSWORD=change_me_secure_password

# Image Docker à utiliser (depuis le registry GitLab)
# Exemple : registry.gitlab.com/votre_user/achat:latest
DOCKER_IMAGE=registry.gitlab.com/VOTRE_UTILISATEUR/achat:latest
